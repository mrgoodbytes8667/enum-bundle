{
    "name": "mrgoodbytes8667/enum-serializer-bundle",
    "description": "A bundle to enable spatie/enum Serialization support in Symfony",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "cerbero/enum": "^2.2",
        "illuminate/collections": "^9.13 | ^10.0 | ^11.0",
        "symfony/config": "^6.3 | ^7.0",
        "symfony/dependency-injection": "^6.3 | ^7.0",
        "symfony/framework-bundle": "^6.3 | ^7.0",
        "symfony/property-access": "^6.3 | ^7.0",
        "symfony/serializer": "^6.3 | ^7.0",
        "symfony/translation-contracts": "^3.5",
        "twig/twig": "^2.12 | ^3.0"
    },
    "require-dev": {
        "ext-json": "*",
        "fakerphp/faker": "^1.13",
        "friendsofphp/php-cs-fixer": "^3.58",
        "rector/rector": "^2.0",
        "symfony/test-pack": "^1.0.9",
        "symfony/var-dumper": "^6.3 | ^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "<9.5"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\EnumSerializerBundle\\": ""
        }
    },
    "minimum-stability": "dev"
}
