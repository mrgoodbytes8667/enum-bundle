{
    "name": "mrgoodbytes8667/enum-serializer-bundle",
    "description": "A bundle to enable spatie/enum Serialization support in Symfony",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "require": {
        "php": "^8.1",
        "sensio/framework-extra-bundle": "^5.1 | ^6.0",
        "symfony/config": "^5.2 | ^6.0",
        "symfony/dependency-injection": "^5.2 | ^6.0",
        "symfony/framework-bundle": "^5.2 | ^6.0",
        "symfony/property-access": "^5.2 | ^6.0",
        "symfony/serializer": "^5.2 | ^6.0"
    },
    "require-dev": {
        "ext-json": "*",
        "fakerphp/faker": "^1.13",
        "symfony/test-pack": "^1.0.9",
        "symfony/var-dumper": "^5.2 | ^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "<9.5"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\EnumSerializerBundle\\": ""
        }
    },
    "minimum-stability": "dev"
}
