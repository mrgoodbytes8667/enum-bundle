{
    "name": "mrgoodbytes8667/enum-serializer-bundle",
    "description": "A bundle to enable spatie/enum Serialization support in Symfony",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "sensio/framework-extra-bundle": "^5.6",
        "spatie/enum": "^3.2|^3.3|^3.4|^3.5",
        "symfony/config": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/property-access": "^5.1",
        "symfony/serializer": "^5.1"
    },
    "require-dev": {
        "ext-json": "*",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/var-dumper": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bytes\\EnumSerializerBundle\\": ""
        }
    }
}
